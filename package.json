{
  "name": "hooknode",
  "version": "0.1.0",
  "description": "",
  "scripts": {
    "start": "NODE_ENV=production ./node_modules/nodemon/bin/nodemon.js ./src/server.js --exec babel-node --presets es2015,stage-2",
    "watch": "./node_modules/nodemon/bin/nodemon.js ./src/server.js --exec babel-node --presets es2015,stage-2",
